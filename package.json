{
  "name": "@schenatosoft/react-native-credit-card",
  "version": "1.0.1",
  "description": "A simple and fully customizable React Native component that implements a Credit Card UI.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "test": "jest --config jestconfig.json --passWithNoTests",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schenatosoft/react-native-credit-card.git"
  },
  "keywords": [
    "react-native",
    "react component",
    "react native component",
    "react",
    "react native",
    "mobile",
    "ios",
    "android",
    "ui",
    "credit card",
    "card",
    "credit-card"
  ],
  "author": "Mateus Schenato <contato@schenato.com.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schenatosoft/react-native-credit-card/issues"
  },
  "homepage": "https://github.com/schenatosoft/react-native-credit-card#readme",
  "devDependencies": {
    "@types/react": "^16.9.53",
    "@types/react-native": "^0.63.30",
    "react": "^17.0.1",
    "react-native": "^0.63.3",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2"
  }
}
