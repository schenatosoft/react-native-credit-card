  "description": "A simple and fully customizable React Native component that implements a Credit Card UI.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc --project ./tsconfig.json",
    "test": "jest --config jestconfig.json --passWithNoTests",
