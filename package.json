    "@types/react-native": "^0.63.30",
    "@types/styled-components": "^5.1.4",
    "jest": "^26.6.1",
    "react": "^17.0.1",
    "react-native": "^0.63.3",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "card-validator": "^8.1.0",
