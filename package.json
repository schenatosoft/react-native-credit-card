    "credit card",
    "card",
    "credit-card"
  ],
  "author": {
    "name": "Mateus Schenato",
    "email": "contato@schenato.com.br",
    "url": "https://schenato.com.br"
  },
  "contributors": [
    {
      "name": "Mateus Schenato",
